{
  "name": "ember-load",
  "version": "0.0.12",
  "description": "Spinner while your ember.js app loads",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@ember/optional-features": "0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.5.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-github-pages": "0.2.1",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-disable-proxy-controllers": "1.0.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "^4.1.2",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.0",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "^5.3.0",
    "eslint-plugin-node": "^8.0.0",
    "husky": "1.1.3",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/mike-north/ember-load.git",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-version-checker": "^2.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
