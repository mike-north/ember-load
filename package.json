{
  "name": "ember-load",
  "version": "0.0.12",
  "description": "Spinner while your ember.js app loads",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.6",
    "@commitlint/config-conventional": "8.3.6",
    "@commitlint/travis-cli": "8.3.6",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.3.1",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "4.9.2",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars-inline-precompile": "3.0.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-template-lint": "1.0.0-beta.3",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-disable-proxy-controllers": "1.0.2",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "6.1.1",
    "ember-resolver": "9.0.1",
    "ember-source": "4.10.0",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.4.0",
    "eslint-plugin-ember": "11.4.2",
    "eslint-plugin-ie11": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "husky": "2.7.0",
    "loader.js": "4.7.0",
    "qunit-dom": "2.0.0",
    "semantic-release": "15.12.5"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/mike-north/ember-load.git",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-version-checker": "^3.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
